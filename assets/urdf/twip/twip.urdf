<?xml version="1.0" encoding="utf-8"?>
<robot name="twip">
    <link name="body">
        <visual>
            <origin xyz="0 0 -0.205" rpy="0 -0 0" />
            <geometry>
                <mesh filename="meshes/body.stl" />
            </geometry>
            <material name="body_material">
                <color rgba="0.615686 0.811765 0.929412 1.0" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 -0.205" rpy="0 -0 0" />
            <geometry>
                <mesh filename="meshes/body.stl" />
            </geometry>
            <material name="body_material">
                <color rgba="0.615686 0.811765 0.929412 1.0" />
            </material>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="2.0" />
            <inertia ixx="1e-3" ixy="0" ixz="0" iyy="1e-3" iyz="0" izz="1e-3" />
        </inertial>
    </link>

    <link name="centre">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0" />
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
        </inertial>
    </link>
    <joint name="centre_frame" type="fixed">
        <origin xyz="0 0 -0.205" rpy="0 -0 0" />
        <parent link="body" />
        <child link="centre" />
        <axis xyz="0 0 0" />
    </joint>

    <link name="left_wheel">
        <visual>
            <origin xyz="0 0.205 0.155" rpy="-1.5708 -1.5708 0" />
            <geometry>
                <mesh filename="meshes/left_wheel.stl" />
            </geometry>
            <material name="left_wheel_material">
                <color rgba="0.647059 0.647059 0.647059 1.0" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 0.205 0.155" rpy="-1.5708 -1.5708 0" />
            <geometry>
                <mesh filename="meshes/left_wheel.stl" />
            </geometry>
            <material name="left_wheel_material">
                <color rgba="0.647059 0.647059 0.647059 1.0" />
            </material>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0.3" />
            <!-- <inertia ixx="1e-3" ixy="0" ixz="0" iyy="1e-3" iyz="0" izz="1e-3" /> -->
        </inertial>
    </link>

    <link name="right_wheel">
        <visual>
            <origin xyz="0 -0.205 0.155" rpy="1.5708 1.5708 0" />
            <geometry>
                <mesh filename="meshes/right_wheel.stl" />
            </geometry>
            <material name="right_wheel_material">
                <color rgba="0.647059 0.647059 0.647059 1.0" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 -0.205 0.155" rpy="1.5708 1.5708 0" />
            <geometry>
                <mesh filename="meshes/right_wheel.stl" />
            </geometry>
            <material name="right_wheel_material">
                <color rgba="0.647059 0.647059 0.647059 1.0" />
            </material>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0.3" />
            <!-- <inertia ixx="1e-3" ixy="0" ixz="0" iyy="1e-3" iyz="0" izz="1e-3" /> -->
        </inertial>
    </link>

    <joint name="lwheel" type="revolute">
        <origin xyz="-0.155 0 -0.41" rpy="1.5708 -0 1.5708" />
        <parent link="body" />
        <child link="left_wheel" />
        <axis xyz="0 0 1" />
        <limit effort="1"/>
        <joint_properties friction="1.0" />
        <dynamics damping="0.0" friction="1.0"/>
    </joint>

    <joint name="rwheel" type="revolute">
        <origin xyz="0.155 0 -0.41" rpy="-1.5708 -0 1.5708" />
        <parent link="body" />
        <child link="right_wheel" />
        <axis xyz="0 0 1" />
        <limit effort="1"/>
        <joint_properties friction="1.0" />
        <dynamics damping="0.0" friction="1.0"/>
    </joint>
</robot>